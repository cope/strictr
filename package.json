{
	"name": "strictr",
	"version": "0.5.1",
	"description": "strict statement checker",
	"engines": {
		"node": ">=20.12.1"
	},
	"repository": "https://github.com/cope/strictr.git",
	"author": "Predrag Stojadinovic <Predrag@Stojadinovic.net>",
	"license": "MIT",
	"preferGlobal": true,
	"main": "./lib/strictr.js",
	"types": "./lib/strictr.d.ts",
	"bin": {
		"strictr": "./lib/strictr.js"
	},
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"go": "ts-node ./src/strictr.ts",
		"all": "pnpm i & pnpm dedupe & pnpm audit --fix & tsc & pnpm clean & pnpm format & pnpm build & pnpm test & pnpm docs",
		"build": "del /S /Q lib\\* && tsc",
		"build:watch": "tsc --watch",
		"lint": "eslint src/**/*.ts test/**/*.ts",
		"lint:fix": "eslint src/**/*.ts test/**/*.ts --fix",
		"docs": "typedoc --readme none --out docs src",
		"test": "jest --coverage --config jest.config.js",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"clean": "eslint src/**/*.ts test/**/*.ts --fix",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"up": "pnpm up -i",
		"upgrade": "pnpm up",
		"latest": "pnpm up -i -L",
		"l": "pnpm i & pnpm latest"
	},
	"dependencies": {
		"@types/cli-color": "^2.0.6",
		"@types/cli-table": "^0.3.4",
		"cli-color": "^2.0.4",
		"cli-table": "^0.3.11",
		"commander": "^14.0.0",
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/lodash": "^4.17.20",
		"@types/node": "^24.0.14",
		"@typescript-eslint/eslint-plugin": "^8.37.0",
		"@typescript-eslint/parser": "^8.37.0",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-jest": "^29.0.1",
		"eslint-plugin-prettier": "^5.5.1",
		"jest": "^30.0.4",
		"prettier": "^3.6.2",
		"source-map-support": "^0.5.21",
		"ts-jest": "^29.4.0",
		"ts-node": "^10.9.2",
		"typedoc": "^0.28.7",
		"typescript": "^5.8.3"
	},
	"pnpm": {
		"updateConfig": {
			"ignoreDependencies": []
		},
		"onlyBuiltDependencies": [
			"es5-ext"
		],
		"overrides": {
			"@babel/helpers@<7.26.10": ">=7.26.10",
			"brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12"
		}
	},
	"packageManager": "pnpm@10.13.1"
}
